{
  "name": "plode-docs-nextra",
  "type": "module",
  "author": "Plode Dev Team",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "next build",
    "build:prod": "next build; pnpm postbuild",
    "clean": "rimraf .next .turbo",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next -p 3030",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs; node generate-language-sitemaps.cjs; node generate-sitemap-index.cjs",
    "start": "next start -p 3030",
    "types:check": "tsc --noEmit",
    "fmt": "prettier --write '**/*.(css|tsx|ts|jsx|js|cjs|mjs|json|md|html)'",
    "lint": "prettier --check '**/*.(css|tsx|ts|jsx|js|cjs|mjs|json|md|html)'"
  },
  "dependencies": {
    "@graphiql/react": "^0.37.1",
    "@graphiql/toolkit": "^0.11.3",
    "@next/third-parties": "^15.5.6",
    "@tanstack/react-query": "^5.90.5",
    "@vercel/og": "^0.8.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "firebase": "^12.4.0",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "next": "^15.5.6",
    "next-sitemap": "^4.2.3",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-intersection-observer": "^9.16.0",
    "react-select": "^5.10.2",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@csstools/postcss-oklab-function": "^4.0.12",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "shiki": "^3.13.0",
    "tailwindcss": "3.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
